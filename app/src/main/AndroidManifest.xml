<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.consolidated.myapp.consolidated">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextViewShowcase" />
        <activity android:name=".ButtonShowcase" />
        <activity android:name=".ActivityInformationTransferShowcase" />
        <activity android:name=".HiddenActivity" />
        <activity android:name=".AlertDialogShowcase" />
        <activity android:name=".SeekBarShowcase" />
        <activity android:name=".MediaPlayerShowcase"></activity>
    </application>

</manifest>